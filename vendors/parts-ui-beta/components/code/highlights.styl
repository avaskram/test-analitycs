parts-ui-code-dark()
  +after()
    gradient-dark-right()

  .hljs
    display block
    overflow-x auto
    padding 0.5em
    color $gray
    bg-dark()

  .hljs-comment,
  .hljs-quote
    color $gray-sat
    font-style: italic
  .hljs-doctag,
  .hljs-keyword,
  .hljs-formula
    color $purple
  .hljs-section,
  .hljs-name,
  .hljs-selector-tag,
  .hljs-deletion,
  .hljs-subst
    color darken(saturation($red,55),5)
  .hljs-literal
    color $tale
  .hljs-string,
  .hljs-regexp,
  .hljs-addition,
  .hljs-attribute,
  .hljs-meta-string
    color lighten(saturation($green,30),5)
  .hljs-built_in,
  .hljs-class .hljs-title
    color $yellow
  .hljs-attr,
  .hljs-variable,
  .hljs-template-variable,
  .hljs-type,
  .hljs-selector-class,
  .hljs-selector-attr,
  .hljs-selector-pseudo,
  .hljs-number
    color: saturation($orange,60)
  .hljs-symbol,
  .hljs-bullet,
  .hljs-link,
  .hljs-meta,
  .hljs-selector-id,
  .hljs-title
    color $blue
  .hljs-emphasis
    font-style: italic
  .hljs-strong
    font-weight: bold
  .hljs-link
    text-decoration: underline

parts-ui-code-light()
  +after()
    gradient-color-right($gray-bg)

  .hljs
    display block
    overflow-x auto
    -webkit-overflow-scrolling touch
    padding 0.5em
    color $gray-dark
    position relative
    background-color $gray-bg

  .hljs-comment,
  .hljs-quote
    color $gray-light
    font-style italic


  .hljs-doctag,
  .hljs-keyword,
  .hljs-formula
    color: $purple
  .hljs-section,
  .hljs-name,
  .hljs-selector-tag,
  .hljs-deletion,
  .hljs-subst
    color: darken(saturation($red,50),3)
  .hljs-literal
    color: $info-color
  .hljs-string,
  .hljs-regexp,
  .hljs-addition,
  .hljs-attribute,
  .hljs-meta-string
    color: darken(saturation($green,50),3)
  .hljs-built_in,
  .hljs-class .hljs-title
    color: $yellow
  .hljs-attr,
  .hljs-variable,
  .hljs-template-variable,
  .hljs-type,
  .hljs-selector-class,
  .hljs-selector-attr,
  .hljs-selector-pseudo,
  .hljs-number
    color: saturation($orange,60)
  .hljs-symbol,
  .hljs-bullet,
  .hljs-link,
  .hljs-meta,
  .hljs-selector-id,
  .hljs-title
    color: $info-color
  .hljs-emphasis
    font-style: italic
  .hljs-strong
    font-weight: bold
  .hljs-link
    text-decoration: underline
